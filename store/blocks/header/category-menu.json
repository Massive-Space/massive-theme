{
  "vtex.menu@2.x:menu#category-menu": {
    "props": {
      "experimentalOptimizeRendering": true
    },
    "children": [
      "menu-item#more"
    ]
  },

  "menu-item#more": {
    "props": {
      "id": "menu-item-custom-sale",
      "type": "custom",
      "iconId": null,
      "highlight": false,
      "onMountBehavior": "closed",
      "itemProps": {
        "type": "internal",
        "href": "#",
        "noFollow": true,
        "tagTitle": "Habitación",
        "text": "Habitación"
      }
    },
    "blocks": [
      "vtex.menu@2.x:submenu#more"
    ]
  },
  "vtex.menu@2.x:submenu#more": {
    "props": {
      "width": "100%"
    },
    "children": [
      "flex-layout.row#menu-more"
    ]
  },
  "flex-layout.row#menu-more": {
    "children": [
      "flex-layout.col#menu-more-1",
      "flex-layout.col#menu-more-2"
    ]
  },
  "flex-layout.col#menu-more-1": {
    "children": [
      "vtex.menu@2.x:menu#more"
    ]
  },
  "flex-layout.col#menu-more-2": {
    "children": [
      "flex-layout.row#category-images"
    ]
  },
  "flex-layout.row#category-images": {
    "children": [
      "flex-layout.col#category-image-1",
      "flex-layout.col#category-image-1",
      "flex-layout.col#category-image-1",
      "flex-layout.col#category-image-1"
    ]
  },

  "flex-layout.col#category-image-1": {
    "children": [
      "image#example",
      "rich-text#example"
    ]
  },

  "image#example": {
    "props": {
      "src": "https://storecomponents.vteximg.com.br/arquivos/box.png",
      "maxHeight": 24
    }
  },

  "rich-text#example": {
    "props": {
      "textAlignment": "CENTER",
      "textPosition": "CENTER",
      "text": "Visit our [help](https://developers.vtex.com/vtex-developer-docs/docs/welcome) section.\n**Be Bold!**\n*This is italic*",
      "textColor": "c-on-emphasis",
      "font": "t-heading-5",
      "blockClass": "help-message"
    }
  },



  "vtex.menu@2.x:menu#more": {
    "props": {
      "orientation": "vertical"
    },
    "children": [
      "menu-item#almohadas",
      "menu-item#cobijas"
    ]
  },
  "menu-item#almohadas": {
    "props": {
      "id": "menu-item-almohadas",
      "type": "custom",
      "iconId": null,
      "highlight": false,
      "itemProps": {
        "type": "internal",
        "href": "/Almohadas",
        "noFollow": true,
        "tagTitle": "Almohadas",
        "text": "Almohadas"
      }
    }
  },
  "menu-item#cobijas": {
    "props": {
      "id": "menu-item-cobijas",
      "type": "custom",
      "iconId": null,
      "highlight": false,
      "itemProps": {
        "type": "internal",
        "href": "/Cobijas",
        "noFollow": true,
        "tagTitle": "Cobijas",
        "text": "Cobijas"
      }
    }
  }
}
